<BorderPane fx:id="MainComponent" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.iup.tp.twitup.core.viewControllers.MainController">
   <left>
      <GridPane fx:id="GridPaneAccueilLeft" BorderPane.alignment="CENTER">
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         </rowConstraints>
         <children>
            <Button fx:id="btnAccueil" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onBtnAccueilAction" prefHeight="76.0" prefWidth="139.0" text="Accueil" GridPane.hgrow="ALWAYS" />
            <Button fx:id="btnUsers" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onBtnUtsersAction" prefHeight="78.0" prefWidth="149.0" text="Utilisateurs" GridPane.rowIndex="1" />
            <Button fx:id="btnProfile" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onBtnProfileAction" prefHeight="98.0" prefWidth="170.0" text="Profil" GridPane.rowIndex="2" />
            <Button fx:id="btnNewTweet" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onBtnNewTweetAction" prefHeight="123.0" prefWidth="222.0" text="Nouveau tweet" GridPane.rowIndex="3" />
            <Button fx:id="btnResearch" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onBtnResearchAction" prefHeight="145.0" prefWidth="246.0" text="Rechercher" GridPane.rowIndex="4" />
            <Button fx:id="btnDisconnect" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onBtnDisconnectAction" prefHeight="189.0" prefWidth="258.0" text="Deconnexion" GridPane.rowIndex="5" />
         </children>
      </GridPane>
   </left>
   <center>
      <Pane fx:id="PaneAccueilCenter" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </center>
   <right>
      <Pane fx:id="PaneAccueilRight" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </right>
</BorderPane>
