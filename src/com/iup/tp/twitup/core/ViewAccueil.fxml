<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>

<BorderPane fx:id="MainComponent" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.iup.tp.twitup.core.viewControllers.MainController">
   <left>
      <GridPane fx:id="GridPaneAccueilLeft" BorderPane.alignment="CENTER">
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         </rowConstraints>
         <children>
            <Button fx:id="btnAccueil" mnemonicParsing="false" onAction="#onBtnAccueilAction" prefHeight="127.0" prefWidth="207.0" text="Accueil" />
            <Button fx:id="btnUsers" mnemonicParsing="false" onAction="#onBtnUtsersAction" prefHeight="104.0" prefWidth="149.0" text="Utilisateurs" GridPane.rowIndex="1" />
            <Button fx:id="btnProfile" mnemonicParsing="false" onAction="#onBtnProfileAction" prefHeight="148.0" prefWidth="234.0" text="Profil" GridPane.rowIndex="2" />
            <Button fx:id="btnNewTweet" mnemonicParsing="false" onAction="#onBtnNewTweetAction" prefHeight="117.0" prefWidth="199.0" text="Nouveau tweet" GridPane.rowIndex="3" />
            <Button fx:id="btnResearch" mnemonicParsing="false" onAction="#onBtnResearchAction" prefHeight="119.0" prefWidth="244.0" text="Rechercher" GridPane.rowIndex="4" />
            <Button fx:id="btnDisconnect" mnemonicParsing="false" onAction="#onBtnDisconnectAction" prefHeight="139.0" prefWidth="386.0" text="Deconnexion" GridPane.rowIndex="5" />
         </children>
      </GridPane>
   </left>
   <center>
      <Pane fx:id="PaneAccueilCenter" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </center>
   <right>
      <Pane fx:id="PaneAccueilRight" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </right>
</BorderPane>
